{
  "name": "restackx-cli",
  "version": "0.3.2",
  "description": "cli tool for restackx",
  "bin": {
    "restackx": "./bin/restackx",
    "restackx-run": "./bin/restackx-run",
    "restackx-build": "./bin/restackx-build",
    "restackx-create": "./bin/restackx-create"
  },
  "scripts": {
    "compile": "babel -d lib/ src/",
    "watch": "babel src --watch --out-dir lib --stage=0",
    "prepublish": "npm run compile"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-decorators",
      "transform-runtime",
      "syntax-async-functions",
      "transform-regenerator",
      "transform-object-assign"
    ]
  },
  "author": "hibad",
  "license": "MIT",
  "dependencies": {
    "archiver": "^2.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "body-parser": "^1.17.2",
    "browser": "^0.2.6",
    "commander": "^2.11.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra-promise": "^1.0.1",
    "fs-tree-traverse": "^0.1.0",
    "happypack": "^4.0.0-beta.1",
    "highlight.js": "^9.12.0",
    "history": "^4.6.3",
    "html-webpack-harddisk-plugin": "0.1.0",
    "html-webpack-plugin": "^2.29.0",
    "isomorphic-fetch": "^2.2.1",
    "jed": "^1.1.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mustache": "^2.3.0",
    "ncp": "^2.0.0",
    "postcss-less": "^1.1.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "unzip": "^0.1.11",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "devDependencies": {}
}
